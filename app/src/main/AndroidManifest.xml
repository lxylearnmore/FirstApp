<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mylord.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TableActivity"></activity>
        <activity android:name=".ConstraintActivity" />
        <activity android:name=".LinearActivity" />
        <activity android:name=".MainActivity"> <!-- 在清单文件中声明你的Activity，这样系统才能访问它 -->
            <intent-filter> <!-- 声明其他应用组件激活它的方法 -->
                <action android:name="android.intent.action.MAIN" />
                <!-- action元素指定这是应用的“主”入口点 -->
                <category android:name="android.intent.category.LAUNCHER" />
                <!-- 指定次Activity应列入系统的应用启动器内 -->
            </intent-filter>
        </activity>
    </application>

</manifest>